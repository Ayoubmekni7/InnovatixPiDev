<!DOCTYPE html>

<title>Article</title>

{% block body %}
    <h1>Article</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ article.id }}</td>
            </tr>
            <tr>
                <th>NomAutArt</th>
                <td>{{ article.nomAutArt }}</td>
            </tr>
            <tr>
                <th>AdrAutArt</th>
                <td>{{ article.adrAutArt }}</td>
            </tr>
            <tr>
                <th>DatePubArt</th>
                <td>{{ article.datePubArt ? article.datePubArt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>DureeArt</th>
                <td>{{ article.dureeArt }}</td>
            </tr>
            <tr>
                <th>CategorieArt</th>
                <td>{{ article.categorieArt }}</td>
            </tr>
            <tr>
                <th>TitreArt</th>
                <td>{{ article.titreArt }}</td>
            </tr>
            <tr>
                <th>ContenuArt</th>
                <td>{{ article.contenuArt }}</td>
            </tr>
            <tr>
                <th>PiecejointeArt</th>
                <td>{{ article.piecejointeArt }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_article_index') }}">back to list</a>

    <a href="{{ path('app_article_edit', {'id': article.id}) }}">edit</a>

    {{ include('article/_delete_form.html.twig') }}
{% endblock %}
